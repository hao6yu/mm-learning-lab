# AI Proxy (recommended for production)
AI_PROXY_BASE_URL=http://localhost:8787
AI_PROXY_TOKEN=replace_with_proxy_token
AI_PROXY_REQUIRED=true
AI_ALLOW_DIRECT_FALLBACK=false

# Optional direct provider keys (development fallback only)
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# OpenAI Models
OPENAI_CHAT_MODEL=gpt-4.1
OPENAI_CHAT_MINI_MODEL=gpt-4.1-mini
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview
OPENAI_REASONING_MODEL=o3
