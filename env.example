# AI Proxy (recommended for production)
AI_PROXY_BASE_URL=http://localhost:8787
AI_PROXY_TOKEN=replace_with_proxy_token
AI_PROXY_REQUIRED=true
AI_ALLOW_DIRECT_FALLBACK=false

# Optional direct provider keys (development fallback only)
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id_here
ELEVENLABS_AGENT_USE_SIGNED_URL=true

# Optional direct websocket override for public agents.
# Example: wss://api.elevenlabs.io/v1/convai/conversation?agent_id=agent_xxx
ELEVENLABS_AGENT_WEBSOCKET_URL=

# OpenAI Models
OPENAI_CHAT_MODEL=gpt-5-mini
OPENAI_CHAT_MINI_MODEL=gpt-5-nano
OPENAI_REASONING_MODEL=o3
